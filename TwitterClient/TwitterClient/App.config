<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <appSettings>
    <!-- Twitter Streaming API settings.  Replace Twitter OAuth settings with your own -->
    <add key="oauth_consumer_key" value="[ENTER_CONSUMER_KEY]" />
    <add key="oauth_consumer_secret" value="[ENTER_CONSUMER_SECRET]" />
    <add key="oauth_token" value="[ENTER_AUTH_TOKEN]" />
    <add key="oauth_token_secret" value="[ENTER_AUTH_TOKEN_SECRET]" />
  
    
    <!-- Enter keywords, hashtags, or handles here.  Use comma to separate multiple entries -->
    <add key="twitter_keywords" value="[ENTER_KEYWORD]" />
    
    <!-- Service Bus specific app setings for messaging connections -->
    <!-- CONNECTION STRING FORMAT: Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=<sas policy name>;SharedAccessKey=<sas key>-->
    <add key="EventHubConnectionString" value="[ENTER_EVENTHUB_CONNECTIONSTRING]" />
    <add key="EventHubName" value="[ENTER_EVENTHUB_NAME]" />
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>